!/bin/bash
export $WAGGLE=.
dpkg -i packages_o/*.deb
dir = .
cd packages_o/
pip install -e pika-0.9.14
cd $dir
mkdir /etc/waggle/
rm /etc/smsc95xx_mac_addr
echo $RANDOM > /etc/waggle/hostname # The node's hostname
cd waggled/DataCache
python Data_Cache.py start #start the Data Data_Cache
cd ..
cd Communications
ls
./background_comms.sh #start the background_comms