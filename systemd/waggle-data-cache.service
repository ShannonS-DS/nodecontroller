[Unit]
Description=Waggle data cache for sensor data. (Deprecated?)
After=network.target

[Service]
WorkingDirectory=/usr/lib/waggle/nodecontroller/nc-wag-os/waggled/DataCache
ExecStart=/usr/lib/waggle/nodecontroller/nc-wag-os/waggled/DataCache/Data_Cache.py --force --logging

Restart=always
RestartSec=5

[Install]
WantedBy=multi-user.target
